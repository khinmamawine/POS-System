	<script src="/Login_v4/vendor/countdowntime/countdowntime.js"></script>
<!--===============================================================================================-->
	<script src="/Login_v4/js/main.js"></script>
	<script type="text/javascript">
		document.querySelector('.login100-form-btn').addEventListener('click',init);
        document.addEventListener('keypress',function(event){

        if(event.keyCode===13 || event.which===13){

            init();
        }
    });
        function init(){
        	var url='file:///E:/POS-system/Admin.html';
        	window.location.replace(url);
        }



	</script>

</body>
