                                <legend><h3><i>Showroom Table Detail</i></h3></legend>
                                    <div class="well" style="overflow: auto; max-height: 400px;">
                                        <form action=""  class="form-horizontal">
                                            <table class="table table-bordered">
                                                <tr>
                                                    <th>Item_Type</th>
                                                    <th>Barcode</th>
                                                    <th>Item_Name</th>
                                                    <th>Brand_Name</th>
                                                    <th>Company_Name</th>
                                                    <th>Purchase_Price</th>
                                                    <th>Sale_Price</th>
                                                    <th>Qty</th>
                                                    <th>Remark</th>
                                                </tr>
                                                <tbody>
                                                <% for (var i = 0; i < showroom.length; i++) {%>
                                                <tr>
                                                <td><%= showroom[i].itemType %></td>
                                                <td><%= showroom[i].barCode %></td>
                                                <td><%= showroom[i].itemName%></td>
                                                <td><%= showroom[i].brandName %></td>
                                                <td><%= showroom[i].companyName %></td>
                                                <td><%= showroom[i].purchasePrice %></td>
                                                <td><%= showroom[i].salePrice %></td>
                                                <td><%= showroom[i].startDate %></td>
                                                <td><%= showroom[i].expDate %></td>
                                                <td><%= showroom[i].Qty %></td>
                                                <td><%= showroom[i].Remark %></td>
                                                </tr>
                                                <%}%>
                                                </tbody>
                                            </table>
                                        </form>
                                    </div>
                            </fieldset>
                        </div>
